{
  "manifest_version": 3,
  "name": "9dm_redirect",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://www.9dmgamemod.com/*",
    "http://www.9dmgamemod.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
